# Backtest Environment Configuration
APP_ENV=backtest

# Database Configuration (Isolated)
DB_HOST=localhost
DB_PORT=5433
DB_NAME=backtest_db
DB_USER=postgres
DB_PASSWORD=password

# Redis Configuration (Isolated)
REDIS_URL=redis://localhost:6380/0

# API Configuration (Isolated)
API_URL=http://localhost:8001
API_AUTH_SECRET=backtest-secret-key

# Backtest Configuration
BACKTEST_START_DATE=2026-01-01
BACKTEST_END_DATE=2026-01-14
BACKTEST_INITIAL_CAPITAL=100000000

# KIS API (거래는 하지 않으므로 필수 아님, 데이터 로드용)
KIS_APP_KEY=${KIS_APP_KEY:-}
KIS_APP_SECRET=${KIS_APP_SECRET:-}
KIS_ACCOUNT=${KIS_ACCOUNT:-}

# Logging
LOG_LEVEL=INFO
PYTHONUNBUFFERED=1
